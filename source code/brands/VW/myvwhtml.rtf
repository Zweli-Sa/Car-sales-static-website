      if (slideIndex > slides.length) \{slideIndex = 1\}    \par
      for (i = 0; i < dots.length; i++) \{\par
        dots[i].className = dots[i].className.replace(" active", "");\par
      \}\par
      slides[slideIndex-1].style.display = "block";  \par
      dots[slideIndex-1].className += " active";\par
      setTimeout(showSlides, 2000); // Change image every 2 seconds\par
